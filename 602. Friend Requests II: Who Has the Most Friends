/* Write your T-SQL query statement below */
WITH cte AS
(
    SELECT
        requester_id AS id
        ,COUNT(*) AS num
    FROM RequestAccepted
    GROUP BY requester_id

    UNION ALL

    SELECT
        accepter_id AS id
        ,COUNT(*) AS num
    FROM RequestAccepted
    GROUP BY accepter_id
), cte2 AS
(
    SELECT
        id
        ,SUM(num) AS num
    FROM cte
    GROUP BY id
)
SELECT 
    TOP 1 id
    ,num
FROM cte2
ORDER BY num DESC
